{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:css-modules/recommended",
        "plugin:react/recommended",
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "css-modules",
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "react/prop-types": 0,
        "css-modules/no-unused-class": [0, { "camelCase": "dashes" }],
        "css-modules/no-undef-class": [0, { "camelCase": "dashes" }],
        "no-unused-vars": 1,
        "require-jsdoc": 0
    },
    "settings": {
        "react": {
            "version": "17.0.2"
        }   
    }
}
