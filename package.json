{
  "name": "crux-forum-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write --config .prettierrc ./src/** ./pages/** ./lib/**/*.ts ./cypress/**/*.ts",
    "check:format": "prettier --check --config .prettierrc ./src/** ./pages/** ./lib/**/*.ts ./cypress/**/*.ts",
    "lint": "eslint --config .eslintrc.json --fix ./pages/**.{ts,tsx} ./src/**/*.{ts,tsx} ./cypress/**/*.ts",
    "check:lint": "eslint --config .eslintrc.json --quiet ./src/**/*.{ts,tsx} ./pages/**/*.{ts,tsx} ./cypress/**/*.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:open": "yarn cypress open",
    "cy:run": "yarn cypress run --browser chrome",
    "e2e:dev:open": "cross-env NEXT_PUBLIC_IMG_URL='' NEXT_PUBLIC_SERVER_URL=http://localhost:3000/api/test/graphql start-server-and-test dev 3000 cy:open",
    "e2e:dev:run": "cross-env NEXT_PUBLIC_IMG_URL='' NEXT_PUBLIC_SERVER_URL=http://localhost:3000/api/test/graphql start-server-and-test dev 3000 cy:run"
  },
