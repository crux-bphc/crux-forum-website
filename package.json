{
  "name": "crux-forum-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "e2e:dev": "cross-env NEXT_PUBLIC_SERVER_URL=http://localhost:3000/api/graphql start-server-and-test dev 3000 cy:open",
    "e2e": "cross-env NEXT_PUBLIC_SERVER_URL=http://localhost:3000/api/graphql yarn build && start-server-and-test start 3000 cy:run",
    "cy:run-ct": "npx cypress run-ct",
    "cy:open-ct": "npx cypress open-ct",
    "coverage": "npx nyc report",
    "format": "prettier --write --config .prettierrc ./src/** ./cypress/**/*.ts",
    "check:format": "prettier --check --config .prettierrc ./src/** ./cypress/**/*.ts",
    "lint": "eslint --config .eslintrc.json --fix ./src/**/*.{ts,tsx} ./cypress/**/*.ts",
    "check:lint": "eslint --config .eslintrc.json --quiet ./src/**/*.{ts,tsx} ./cypress/**/*.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.12",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "graphql": "^15.5.3",
    "next": "10.2.4-canary.19",
    "next-apollo": "^5.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/react": "^5.10.0",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "@graphql-codegen/typescript-react-apollo": "2.3.0",
    "@graphql-tools/webpack-loader": "^6.6.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/cypress": "^8.0.1",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "apollo-server-micro": "^3.3.0",
    "autoprefixer": "^10.3.4",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.4.1",
    "cypress-react-selector": "^2.3.11",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-react": "^7.25.2",
    "html-webpack-plugin": "^5.3.2",
    "micro": "^9.3.4",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-dev-server": "^3.11.2"
  },
  "nyc": {
    "include": [
      "src/*"
    ],
    "exclude": [
      "src/pages/api/*",
      "src/**/*__tests__"
    ],
    "check-coverage": true,
    "branches": 75,
    "lines": 75,
    "functions": 75,
    "statements": 75
  }
}
